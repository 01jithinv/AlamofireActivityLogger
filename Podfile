# Uncomment this line to define a global platform for your project

abstract_target 'alamofire_activity_logger' do
	use_frameworks!
	pod 'Alamofire', '~> 5.9.1'
    pod 'AlamofireActivityLogger', :path => '.'
    
    target 'ios' do
        platform :ios, '14.0'
    end
    
    target 'macos' do
        platform :osx, '14.0'
    end
    
    target 'watchos Extension' do
        platform :watchos, '510'
    end
    
    target 'tvos' do
        platform :tvos, '14.0'
    end
    
end
